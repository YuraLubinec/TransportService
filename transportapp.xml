  <!-- Application Server Libraries -->
  <!-- Register Custom Compiler Taskdefs -->
  <property name="javac2.home" value="${idea.home}/lib"/>
  <path id="javac2.classpath">
    <pathelement location="${javac2.home}/javac2.jar"/>
    <pathelement location="${javac2.home}/jdom.jar"/>
    <pathelement location="${javac2.home}/asm-all.jar"/>
    <pathelement location="${javac2.home}/jgoodies-forms.jar"/>
  </path>
  <target name="register.custom.compilers">
    <taskdef name="javac2" classname="com.intellij.ant.Javac2" classpathref="javac2.classpath"/>
    <taskdef name="instrumentIdeaExtensions" classname="com.intellij.ant.InstrumentIdeaExtensions" classpathref="javac2.classpath"/>
  </target>
  
  <!-- Modules -->
  
  <import file="${basedir}/module_transportapp.xml"/>
  
  <target name="init" description="Build initialization">
    <!-- Perform any build initialization in this target -->
  </target>
  
  <target name="clean" depends="clean.module.transportapp, clean.artifact.transportapp:war_exploded" description="cleanup all"/>
  
  <target name="build.modules" depends="init, clean, compile.module.transportapp" description="build all modules"/>
  
  <target name="init.artifacts">
    <property name="artifacts.temp.dir" value="${basedir}/__artifacts_temp"/>
    <property name="artifact.output.transportapp:war" value="${basedir}/target"/>
    <property name="artifact.output.transportapp:war_exploded" value="${basedir}/target/transportApp"/>
    <mkdir dir="${artifacts.temp.dir}"/>
    <property name="temp.jar.path.transportApp.war" value="${artifacts.temp.dir}/transportApp.war"/>
  </target>
  
  <target name="clean.artifact.transportapp:war_exploded" description="clean transportApp:war exploded artifact output">
    <delete dir="${artifact.output.transportapp:war_exploded}"/>
  </target>
  
  <target name="artifact.transportapp:war" depends="init.artifacts, artifact.transportapp:war_exploded" description="Build &#39;transportApp:war&#39; artifact">
    <property name="artifact.temp.output.transportapp:war" value="${artifacts.temp.dir}/transportApp_war"/>
    <mkdir dir="${artifact.temp.output.transportapp:war}"/>
    <zip destfile="${temp.jar.path.transportApp.war}">
      <zipfileset dir="${artifact.output.transportapp:war_exploded}"/>
    </zip>
